Vagrant.configure("2") do |config|
  config.vm.box = "jokerng-library"
  config.vm.provider "virtualbox" do |vb|
    vb.memory = "2048"
    vb.cpus = "2"
  end
  config.vm.base_mac = "080024214C66"
  config.vm.synced_folder ".", "/vagrant", disabled: true
  config.ssh.shell = "/bin/sh"
end
